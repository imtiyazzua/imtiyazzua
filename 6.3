/*----------------------------------------------------------*/
/*Program to find weather a year is leap tear or not        */
/*----------------------------------------------------------*/
#include<stdio.h>
main()
{
   int year
   printf("Enter a year:");
   scanf("%d",&year);
/*If 'year'is divisiable by 4 AND not divisiable by 100,OR is
divisiable by 400,it is a leap year*/
if (year%4==0&&year%100!=0 || year%400==0)printf("%d is a leap year
\n",year);
else
printf("%d is not a leap year \n",year);
}
